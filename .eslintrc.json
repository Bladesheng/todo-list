{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": "off",
    "prefer-destructuring": "off",
    "no-console": "off",
    "import/no-cycle": "off"
  }
}
